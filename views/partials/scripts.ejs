                event.preventDefault();
                $(this).ekkoLightbox();
            });

</script>

<!-- Scroll top top script make it nicer -->
<script type="text/javascript">
let fixed = false;
$(document).scroll(function() {
  if ($(this).scrollTop() > 250) {
      if (!fixed) {
          fixed = true;
          $('#to-top').show("slow", function() {
              $('#to-top').css({
                  position: 'fixed',
                  display: 'block'
              });
          });
      }
  } else {
      if (fixed) {
          fixed = false;
          $('#to-top').hide("slow", function() {
              $('#to-top').css({
                  display: 'none'
              });
          });
      }
  }
});

</script>
